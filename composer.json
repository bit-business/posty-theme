{
    "version": "2.1.5",
    "name": "skijasi/posty-theme",
    "description": " post module on skijasi",
    "keywords": [
        "laravel",
        "admin",
        "panel",
        "dashboard",
        "pwa",
        "vue",
        "generator",
        "blog",
        "skijasi",
        "blog",
        "news"
    ],
    "license": "proprietary",
    "homepage": "https://skijasi-posty.nadzorservera.co.id/",
    "support": {
        "issues": "https://github.com/nadzorservera-indonesia/skijasi-posty-theme/issues",
        "discussion": "https://github.com/nadzorservera-indonesia/skijasi-posty-theme/discussions",
        "source": "https://github.com/nadzorservera-indonesia/skijasi-posty-theme"
    },
    "type": "library",
    "require": {
        "symfony/var-exporter": "*",
        "skijasi/core": "^2.0",
        "skijasi/post-module": "^2.0",
        "skijasi/content-module": "*",
        "inertiajs/inertia-laravel": "^0.6.3",
        "tightenco/ziggy": "^1.4"
    },
    "authors": [
        {
            "name": "NADZORSERVERA",
            "email": "hello@nadzorservera.co.id"
        }
    ],
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Nadzorservera\\Skijasi\\Theme\\PostyTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nadzorservera\\Skijasi\\Theme\\PostyTheme\\Providers\\PostyThemeProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "orchestra/testbench": "^7.6"
    }
}
