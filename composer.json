{
    "version": "2.1.0",
    "name": "badaso/posty-theme",
    "description": "Official free theme for post module on badaso",
    "keywords": [
        "laravel",
        "admin",
        "panel",
        "dashboard",
        "pwa",
        "vue",
        "generator",
        "blog",
        "badaso",
        "blog",
        "news"
    ],
    "license": "proprietary",
    "homepage": "https://badaso-posty.uatech.co.id/",
    "support": {
        "issues": "https://github.com/uasoft-indonesia/badaso-posty-theme/issues",
        "discussion": "https://github.com/uasoft-indonesia/badaso-posty-theme/discussions",
        "source": "https://github.com/uasoft-indonesia/badaso-posty-theme"
    },
    "type": "library",
    "require": {
        "symfony/var-exporter": "*",
        "badaso/content-module": "*",
        "inertiajs/inertia-laravel": "^0.4.5",
        "tightenco/ziggy": "^1.4"
    },
    "authors": [
        {
            "name": "UASOFT",
            "email": "hello@uatech.co.id"
        }
    ],
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Uasoft\\Badaso\\Theme\\PostyTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uasoft\\Badaso\\Theme\\PostyTheme\\Providers\\PostyThemeProvider"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "badaso/core": "^2.4.10",
        "orchestra/testbench": "*",
        "badaso/post-module": "^2.2.1",
        "badaso/content-module": "^2.0.1"
    }
}
